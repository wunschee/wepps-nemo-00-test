{
	"_version": "1.12.0",
	"sap.app": {
		"id": "pte.grund",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
  	},
	"sap.ui5": {
		"rootView": {
			"viewName": "pte.grund.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.30",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pte.grund.i18n.i18n"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "pte.grund.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "appHome",
				"target": "home"
			}, {
				"pattern": "students",
				"name": "studentList",
				"target": "students"
			}, {
				"pattern": "student/overview:?query:",
				"name": "studentOverview",
				"target": ["StudentOverviewTop", "StudentOverviewContent"]
			}, {
				"pattern": "students/{studentId}",
				"name": "student",
				"target": "student"
			}, {
				"pattern": "students/{studentId}/resume:?query:",
				"name": "studentResume",
				"target": "studentResume"
			}],
			"targets": {
				"home": {
					"viewId": "home",
					"viewName": "Home",
					"viewLevel" : 1
				},
				"notFound": {
					"viewId": "notFound",
					"viewName": "NotFound",
					"transition": "show"
				},
				"students": {
					"viewId": "studentList",
					"viewPath": "pte.grund.view.student",
					"viewName": "StudentList",
					"viewLevel" : 2
				},
				"student": {
					"viewId": "student",
					"viewName": "student.Student",
					"viewLevel" : 3
				},
				"studentResume": {
					"viewId": "resume",
					"viewName": "student.Resume",
					"viewLevel" : 4,
					"transition": "flip"
				},
				"resumeTabHobbies": {
					"parent": "studentResume",
					"viewId": "resumeHobbies",
					"viewPath": "pte.grund.view.student",
					"viewName": "ResumeHobbies",
					"controlId": "hobbiesTab",
					"controlAggregation": "content"
				},
				"resumeTabNotes": {
					"parent": "studentResume",
					"viewId": "resumeNotes",
					"viewPath": "pte.grund.view.student",
					"viewName": "ResumeNotes",
					"controlId": "notesTab",
					"controlAggregation": "content"
				},
				"StudentOverview": {
					"viewId": "studentOverview",
					"viewPath": "pte.grund.view.student.overview",
					"viewName": "StudentOverview",
					"viewLevel" : 2
				},
				"StudentOverviewTop": {
					"parent": "StudentOverview",
					"viewId": "studentOverviewTop",
					"viewPath": "pte.grund.view.student.overview",
					"viewName": "StudentOverviewTop",
					"controlId": "StudentOverviewParent",
					"controlAggregation": "content"
				},
				"StudentOverviewContent": {
					"parent": "StudentOverview",
					"viewId": "studentOverviewContent",
					"viewPath": "pte.grund.view.student.overview",
					"viewName": "StudentOverviewContent",
					"controlId": "StudentOverviewParent",
					"controlAggregation": "content"
				}
			}
		}
	}
}
